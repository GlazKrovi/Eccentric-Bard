datasource db {
  provider = "sqlite"
  url      = "file./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Image {
  id      Int       @id() @default(autoincrement())
  /// @trim
  path    String
  project Project[]
}

model Genre {
  id      Int       @id() @default(autoincrement())
  /// @trim
  label   String
  project Project[]
}

model Project {
  id          Int    @id() @default(autoincrement())
  /// @trim
  title       String
