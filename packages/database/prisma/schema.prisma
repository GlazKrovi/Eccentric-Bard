  coverId     Int
  genreId     Int
  cover       Image  @relation(fields: [coverId], references: [id], onDelete: Cascade, onUpdate: Cascade)
  genre       Genre  @relation(fields: [genreId], references: [id], onDelete: Cascade, onUpdate: Cascade)
}
